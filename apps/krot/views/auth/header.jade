- var version = '?ver=1.0'

meta(charset="utf-8")

if content_title
	title= 'STARTGAZER | ' + content_title
if content_description
	meta(name='description', content=content_description)

link(rel='stylesheet', type='text/css', href='/build/' + __app_name +  '/css/fonts.css' + version)
link(rel='stylesheet', type='text/css', href='/libs/css/txtbirds.css' + version)
block content_style

script(type='text/javascript', src='/libs/js/jquery-2.2.4.min.js' + version)
script(type='text/javascript', src='/build/' + __app_name +  '/js/auth.js' + version)
block content_scripts
