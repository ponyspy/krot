- var version = '?ver=1.0'

meta(charset='utf-8')
meta(name='viewport', content='width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no')
link(rel='shortcut icon', href='/stuff/' + __app_name + '/icons/favicon.ico', type='image/x-icon')

if content_title
	title= content_title
if content_description
	meta(name='description', content=content_description)

link(rel='stylesheet', type='text/css', href='/build/' + __app_name +  '/css/global.css' + version)
block content_style

script(type='text/javascript', src='/libs/js/jquery-2.2.4.min.js' + version)
block content_scripts